{
  "name": "gitmod",
  "version": "0.1.0",
  "description": "Cli for installing github repository in your project",
  "main": "index.js",
  "bin": {
    "gitmod": "gitmod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjoydip/gitmod.git"
  },
  "author": {
    "name": "Joydip Roy",
    "email": "joydipand@gmail.com"
  },
  "engines": {
    "node": ">=6"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "start": "node index",
    "lint": "./node_modules/.bin/eslint **/*.js --ignore-path .gitignore",
    "clean": "rm -rf node_modules && npm i",
		"build": "npm run uninstall -g gitmod && npm i -g .",
		"setup": "npm run build",
    "precommit": "npm run lint"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "isomorphic-git": "^0.35.0",
    "ora": "^3.0.0",
    "read-pkg": "^4.0.1",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "husky": "^1.0.0-rc.13"
  },
  "readme": "readme.md",
  "bugs": {
    "url": "https://github.com/rjoydip/gitmod/issues"
  },
  "homepage": "https://github.com/rjoydip/gitmod#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
